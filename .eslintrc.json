{
  "env": {
    "browser": true,
    "es2021": false
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "eqeqeqeq": "error",
    "curly": "error",
    "no-throw-literal": "warn",
    "prefer-const": "warn",
    "no-var-requires": "warn"
  },
  "settings": {
    "resolveJsonModules": false
  }
}
